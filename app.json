{
  "expo": {
    "name": "Puzzle Pop!",
    "slug": "puzzle-popit",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#B3E5FC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brianfuller.puzzlepopit"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#B3E5FC"
      },
      "package": "com.brianfuller.puzzlepopit"
    },
    "web": {
      "bundler": "metro"
    },
    "description": "A delightful puzzle game for young players with shape puzzles, pop-it bubbles, and balloon popping fun!",
    "primaryColor": "#4CAF50"
  }
}
